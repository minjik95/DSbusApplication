    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        xmlns:android="http://schemas.android.com/apk/res/android">


        <FrameLayout
            android:id="@+id/frameLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <View
                android:id="@+id/line1"
                android:layout_width="8dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@color/colorDS" />

            <ImageView
                android:id="@+id/busStop"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginTop="36dp"
                android:layout_marginLeft="3dp"
                android:background="@drawable/bus_stop" />

            <ImageView
                android:id="@+id/busImage2"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginLeft="1dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/bus"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/plainNo2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/busImage2"
                android:text="" />


            <ImageView
                android:id="@+id/busImage1"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="36dp"
                android:layout_marginLeft="1dp"
                android:visibility="invisible"
                android:background="@drawable/bus" />

            <TextView
                android:id="@+id/plainNo1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/busImage1"
                android:text="" />

        </FrameLayout>

        <TextView
            android:id="@+id/stNm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/frameLayout"
            android:textSize="16dp"
            android:textColor="#000000"
            android:padding="10dp"/>

        <TextView
            android:id="@+id/arsId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/stNm"
            android:layout_toRightOf="@+id/frameLayout"
            android:textSize="12dp"
            android:padding="10dp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#A6A6A6"
            android:layout_marginTop="80dp"
            android:layout_toRightOf="@+id/frameLayout"/>

    </RelativeLayout>

